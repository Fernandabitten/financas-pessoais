# Ambiente
NODE_ENV=development                      # development ou production

# Sessões
SESSION_SECRET=sua_chave_secreta          # Segredo utilizado para sessões

# API IA
GEMINI_API_KEY=sua_chave_api_gemini       # Chave da API Gemini para IA

# Banco de Dados PostgreSQL (Aiven)
DB_HOST=sua_url_do_postgresql             # Exemplo: pg-xxxx.aivencloud.com
DB_PORT=5432                              # Porta padrão do PostgreSQL (ajuste caso necessário)
DB_USER=seu_usuario_aiven                 # Nome de usuário do seu banco
DB_PASSWORD=sua_senha_aiven               # Senha do banco de dados
DB_NAME=seu_nome_banco                    # Nome do banco de dados a ser usado
